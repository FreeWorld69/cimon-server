<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Favicons -->
    <link rel="icon" type="image/png" href="/src/assets/icon/favicon-32x32.png" sizes="32x32">
    <link rel="apple-touch-icon" href="/src/assets/icon/favicon-32x32.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/src/assets/icon/apple-touch-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/src/assets/icon/apple-touch-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/src/assets/icon/apple-touch-icon-144x144.png">
    <link rel="stylesheet" href="/src/assets/css/main.css">

    <title>{{title}}</title>
    {{#block "header-addons"}}{{/block}}
</head>
<body class="body">
    {{#unless dontShowHeader}}
        {{> includes/header }}
    {{/unless}}

    {{#block "content"}}{{/block}}

    {{#unless dontShowFooter}}
        {{> includes/footer }}
    {{/unless}}

    <!-- JS -->
    <script src="/src/assets/js/jquery-3.3.1.min.js"></script>
    <script src="/src/assets/js/bootstrap.bundle.min.js"></script>
    <script src="/src/assets/js/owl.carousel.min.js"></script>
    <script src="/src/assets/js/jquery.mousewheel.min.js"></script>
    <script src="/src/assets/js/jquery.mCustomScrollbar.min.js"></script>
    <script src="/src/assets/js/wNumb.js"></script>
    <script src="/src/assets/js/nouislider.min.js"></script>
    <script src="/src/assets/js/plyr.min.js"></script>
    <script src="/src/assets/js/jquery.morelines.min.js"></script>
    <script src="/src/assets/js/photoswipe.min.js"></script>
    <script src="/src/assets/js/photoswipe-ui-default.min.js"></script>
    <script src="/src/assets/js/main.js"></script>
    {{#block "js-includes"}}{{/block}}
</body>
</html>
